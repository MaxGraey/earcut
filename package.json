{
  "name": "earcut",
  "version": "2.1.3",
  "description": "The fastest and smallest JavaScript polygon triangulation library for your WebGL apps",
  "main": "src/earcut.js",
  "unpkg": "dist/earcut.dev.js",
  "jsdelivr": "dist/earcut.dev.js",
  "files": [
    "dist/earcut.min.js",
    "dist/earcut.dev.js"
  ],
  "scripts": {
    "test": "eslint src test/test.js && tape test/test.js",
    "watch": "mkdirp dist && watchify -v -d src/earcut.js -s earcut -o dist/earcut.dev.js",
    "build-dev": "mkdirp dist && browserify -d src/earcut.js -s earcut > dist/earcut.dev.js",
    "build-min": "mkdirp dist && browserify src/earcut.js -s earcut | uglifyjs -c warnings=false -m > dist/earcut.min.js",
    "prepare": "npm run build-dev && npm run build-min",
    "cov": "istanbul cover test/*.js",
    "bench": "node index_node.js",
    "coveralls": "istanbul cover test/*.js && coveralls < ./coverage/lcov.info",
    "rust:node": "cargo build --release --target wasm32-unknown-unknown && wasm-bindgen target/wasm32-unknown-unknown/release/earcut.wasm --out-dir . --nodejs && node index_node.js",
    "rust:browser": "cargo build --release --target wasm32-unknown-unknown && wasm-bindgen target/wasm32-unknown-unknown/release/earcut.wasm --out-dir . && webpack-dev-server",
    "as:node:debug": "npx asc src/assemblyscript/assembly/index.ts -b build/debug/earcut.as.wasm -t build/debug/earcut.as.wat --sourceMap --validate --importMemory --memoryBase 134217728",
    "as:node:release": "npx asc src/assemblyscript/assembly/index.ts -b build/release/earcut.as.wasm -t build/release/earcut.as.wat --sourceMap --validate --importMemory -O3 --noDebug --noAssert --memoryBase 134217728"
  },
  "author": "Vladimir Agafonkin",
  "license": "ISC",
  "devDependencies": {
    "assemblyscript": "github:AssemblyScript/assemblyscript#c769f65bacd5392e5c9efab112b33244fc0a0c8f",
    "benchmark": "^2.1.4",
    "browserify": "^14.5.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.14.0",
    "eslint-config-mourner": "^2.0.3",
    "istanbul": "^0.4.5",
    "lodash": "^4.17.4",
    "mkdirp": "^0.5.1",
    "tape": "^4.8.0",
    "uglify-js": "^3.3.4",
    "watchify": "^3.9.0",
    "webpack": "^4.0.1",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  },
  "eslintConfig": {
    "extends": "mourner",
    "rules": {
      "no-unmodified-loop-condition": 0
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mapbox/earcut.git"
  }
}
